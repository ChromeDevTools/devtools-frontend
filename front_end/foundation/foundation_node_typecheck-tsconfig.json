{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "composite": true,
    "declaration": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": true,
    "lib": [
      "ES2023",
      "ES2024.Promise",
      "ESNext.Iterator",
      "ESNext.Collection",
      "ESNext.Array"
    ],
    "module": "esnext",
    "noEmit": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": ".",
    "rootDir": "../../../../../front_end/foundation",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2023",
    "tsBuildInfoFile": "foundation_node_typecheck-tsconfig.json.tsbuildinfo",
    "typeRoots": [
      "../../../../../node_modules/@types"
    ],
    "types": [
      "node"
    ],
    "useUnknownInCatchVariables": false
  },
  "files": [
    "../../../../../front_end/foundation/Universe.ts",
    "../../../../../front_end/legacy/legacy-defs.d.ts",
    "../../../../../front_end/global_typings/global_defs.d.ts",
    "../../../../../node_modules/@types/filesystem/index.d.ts"
  ],
  "references": [
    {
      "path": "../core/common/bundle-tsconfig.json"
    },
    {
      "path": "../core/root/bundle-tsconfig.json"
    },
    {
      "path": "../core/sdk/bundle-tsconfig.json"
    },
    {
      "path": "../models/bindings/bundle-tsconfig.json"
    },
    {
      "path": "../models/workspace/bundle-tsconfig.json"
    }
  ]
}