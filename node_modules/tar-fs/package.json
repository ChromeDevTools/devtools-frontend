{
  "name": "tar-fs",
  "version": "3.0.5",
  "description": "filesystem bindings for tar-stream",
  "dependencies": {
    "pump": "^3.0.0",
    "tar-stream": "^3.1.5"
  },
  "optionalDependencies": {
    "bare-fs": "^2.1.1",
    "bare-path": "^2.1.0"
  },
  "imports": {
