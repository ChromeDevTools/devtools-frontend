    "@typescript-eslint/types": "8.47.0",
    "baseline-browser-mapping": "2.8.25",
    "electron-to-chromium": "1.5.248",
    "caniuse-lite": "1.0.30001754",
    "stylelint": {
      "flat-cache": "6.1.12"
    }
  }
